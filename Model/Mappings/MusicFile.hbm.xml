<?xml version="1.0" encoding="UTF-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="Model" 
                   namespace="MusicTool.Model.Domain">

  <class name="MusicFile" table="files">
  	<id name="Id" unsaved-value="0">
		<generator class="native" />
	</id>

	<property name="Path" />
	<property name="Base" />
	<property name="Artist" />
	<property name="Album" />
	<property name="Title" />
	<property name="Size" />
	<property name="Md5sum" />
  </class> 

</hibernate-mapping>